<?xml version="1.0"?>

<rdf:RDF
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xml:base="http://lipidomics.musc.edu/ontology#">

<rdfs:Class rdf:ID="Experiment" />
<rdfs:Class rdf:ID="Source" />
<rdfs:Class rdf:ID="Sample" />
<rdfs:Class rdf:ID="Measurement" />
<rdfs:Class rdf:ID="Characteristic" />
<rdfs:Class rdf:ID="UnitOfMeasurement" />
<rdfs:Class rdf:ID="Value" />

<rdf:Property rdf:ID="hasSource">
<!--  <rdfs:subPropertyOf rdf:resource="" /> -->
  <rdfs:range rdf:resource="Experiment" /> 
  <rdfs:domain rdf:resource="Source" /> 
</rdf:Property>

<rdf:Property rdf:ID="hasSample">
<!--  <rdfs:subPropertyOf rdf:resource="" /> -->
  <rdfs:range rdf:resource="Source" /> 
  <rdfs:domain rdf:resource="Sample" /> 
</rdf:Property>

<rdf:Property rdf:ID="hasMeasurement">
<!--  <rdfs:subPropertyOf rdf:resource="" /> -->
  <rdfs:range rdf:resource="Sample" /> 
  <rdfs:domain rdf:resource="Measurement" /> 
</rdf:Property>

<rdf:Property rdf:ID="hasCharacteristic">
<!--  <rdfs:subPropertyOf rdf:resource="" /> -->
  <rdfs:range rdf:resource="Measurement" /> 
  <rdfs:domain rdf:resource="Characteristic" /> 
</rdf:Property>

<rdf:Property rdf:ID="hasUnitOfMeasure">
<!--  <rdfs:subPropertyOf rdf:resource="" /> -->
  <rdfs:range rdf:resource="Measurement" /> 
  <rdfs:domain rdf:resource="UnitOfMeasure" /> 
</rdf:Property>

<rdf:Property rdf:ID="hasValue">
<!--  <rdfs:subPropertyOf rdf:resource="" /> -->
  <rdfs:range rdf:resource="Measurement" /> 
  <rdfs:domain rdf:resource="Value" /> 
</rdf:Property>


</rdf:RDF>
